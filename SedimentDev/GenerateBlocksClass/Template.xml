<Template>
  <Group Name="Wood">
    <OnBlockInfo>
      <Note><![CDATA['Bark Only']]></Note>
      <Note><![CDATA[if(dataValue < 12, 'Horizontal North/South', null)]]></Note>
      <Note><![CDATA[if(dataValue < 8, 'Horizontal East/West', null)]]></Note>
      <Note><![CDATA[if(dataValue < 4, 'Vertical', null)]]></Note>
      <Note><![CDATA[GetGroupName(0) + ', ' + note]]></Note>
    </OnBlockInfo>

    <BlockInfoCollection Key="orientations">
      <BlockInfo Name="Vertical" DataValue="0"/>
      <BlockInfo Name="HorizontalEastWest" DataValue="4"/>
      <BlockInfo Name="HorizontalNorthSouth" DataValue="8"/>
      <BlockInfo Name="BarkOnly" DataValue="12"/>
    </BlockInfoCollection>

    <OnBlockInfo BlockId="0x11"/>
    <Group Name="Oak">
      <OnBlockInfo DataValue="dataValue + 0"/>
      <Include Key="orientations"/>
    </Group>
    <Group Name="Spruce">
      <OnBlockInfo DataValue="dataValue + 1"/>
      <Include Key="orientations"/>
    </Group>
    <Group Name="Birch">
      <OnBlockInfo DataValue="dataValue + 2"/>
      <Include Key="orientations"/>
    </Group>
    <Group Name="Jungle">
      <OnBlockInfo DataValue="dataValue + 3"/>
      <Include Key="orientations"/>
    </Group>

    <OnBlockInfo BlockId="0xA2"/>
    <Group Name="Acacia">
      <OnBlockInfo DataValue="dataValue + 0"/>
      <Include Key="orientations"/>
    </Group>
    <Group Name="DarkOak">
      <OnBlockInfo DataValue="dataValue + 1"/>
      <Include Key="orientations"/>
    </Group>
  </Group>


  <Group Name="Leaves">
    <OnBlockInfo>
      <Note><![CDATA['Permanent with checks']]></Note>
      <Note><![CDATA[if(dataValue < 12, 'Decaying with checks', null)]]></Note>
      <Note><![CDATA[if(dataValue < 8, 'Permanent with no checks', null)]]></Note>
      <Note><![CDATA[if(dataValue < 4, 'Decaying with no checks', null)]]></Note>
      <Note><![CDATA[GetGroupName(0) + ', ' + note]]></Note>
    </OnBlockInfo>

    <BlockInfoCollection Key="behaviour">
      <BlockInfo Name="Decayable" DataValue="0"/>
      <BlockInfo Name="Permanent" DataValue="4"/>
      <BlockInfo Name="DecayableChecked" DataValue="8"/>
      <BlockInfo Name="PermanentChecked" DataValue="12"/>
    </BlockInfoCollection>

    <OnBlockInfo BlockId="0x12"/>
    <Group Name="Oak">
      <OnBlockInfo DataValue="dataValue + 0"/>
      <Include Key="behaviour"/>
    </Group>
    <Group Name="Spruce">
      <OnBlockInfo DataValue="dataValue + 1"/>
      <Include Key="behaviour"/>
    </Group>
    <Group Name="Birch">
      <OnBlockInfo DataValue="dataValue + 2"/>
      <Include Key="behaviour"/>
    </Group>
    <Group Name="Jungle">
      <OnBlockInfo DataValue="dataValue + 3"/>
      <Include Key="behaviour"/>
    </Group>

    <OnBlockInfo BlockId="0xA1"/>
    <Group Name="Acacia">
      <OnBlockInfo DataValue="dataValue + 0"/>
      <Include Key="behaviour"/>
    </Group>
    <Group Name="DarkOak">
      <OnBlockInfo DataValue="dataValue + 1"/>
      <Include Key="behaviour"/>
    </Group>
  </Group>


  <Group Name="Planks">
    <OnBlockInfo BlockId="0x05" Note="blockInfoName"/>
    <BlockInfo Name="Oak" DataValue="0"/>
    <BlockInfo Name="Spruce" DataValue="1"/>
    <BlockInfo Name="Birch" DataValue="2"/>
    <BlockInfo Name="Jungle" DataValue="3"/>
    <BlockInfo Name="Acacia" DataValue="4"/>
    <BlockInfo Name="DarkOak" DataValue="5"/>
  </Group>


  <Group Name="Sapling">
    <OnBlockInfo BlockId="0x06" Note="blockInfoName"/>
    <BlockInfo Name="Oak" DataValue="0"/>
    <BlockInfo Name="Spruce" DataValue="1"/>
    <BlockInfo Name="Birch" DataValue="2"/>
    <BlockInfo Name="Jungle" DataValue="3"/>
    <BlockInfo Name="Acacia" DataValue="4"/>
    <BlockInfo Name="DarkOak" DataValue="5"/>
  </Group>


  <Group Name="Stone">
    <OnBlockInfo BlockId="0x01" Note="blockInfoName"/>
    <BlockInfo Name="Normal" DataValue="0"/>
    <BlockInfo Name="Granite" DataValue="1"/>
    <BlockInfo Name="PolishedGranite" DataValue="2"/>
    <BlockInfo Name="Diorite" DataValue="3"/>
    <BlockInfo Name="PolishedDiorite" DataValue="4"/>
    <BlockInfo Name="Andesite" DataValue="5"/>
    <BlockInfo Name="PolishedAndesite" DataValue="6"/>
  </Group>


  <Group Name="Dirt">
    <OnBlockInfo BlockId="0x03" Note="blockInfoName"/>
    <BlockInfo Name="Normal" DataValue="0"/>
    <BlockInfo Name="Coarse" DataValue="1"/>
    <BlockInfo Name="Podzol" DataValue="2"/>
  </Group>


  <Group Name="Water">
    <BlockInfoCollection Key="states">
      <For Start="0" Count="8">
        <Name><![CDATA['Horizontal' + ToString(i, 'X')]]></Name>
        <DataValue><![CDATA[i]]></DataValue>
      </For>
      <For Start="8" Count="8">
        <Name><![CDATA['Vertical' + ToString(i, 'X')]]></Name>
        <DataValue><![CDATA[i]]></DataValue>
      </For>
    </BlockInfoCollection>

    <Group Name="Flowing">
      <OnBlockInfo BlockId="0x08"/>
      <Include Key="states"/>
    </Group>
    <Group Name="Stationary">
      <OnBlockInfo BlockId="0x09"/>
      <Include Key="states"/>
    </Group>
  </Group>


  <Group Name="Lava">
    <BlockInfoCollection Key="states">
      <For Start="0" Count="8">
        <Name><![CDATA['Horizontal' + ToString(i, 'X')]]></Name>
        <DataValue><![CDATA[i]]></DataValue>
      </For>
      <For Start="8" Count="8">
        <Name><![CDATA['Vertical' + ToString(i, 'X')]]></Name>
        <DataValue><![CDATA[i]]></DataValue>
      </For>
    </BlockInfoCollection>

    <Group Name="Flowing">
      <OnBlockInfo BlockId="0x0A"/>
      <Include Key="states"/>
    </Group>
    <Group Name="Stationary">
      <OnBlockInfo BlockId="0x0B"/>
      <Include Key="states"/>
    </Group>
  </Group>


  <Group Name="Sand">
    <OnBlockInfo BlockId="0x0C" Note="blockInfoName"/>
    <BlockInfo Name="Normal" DataValue="0"/>
    <BlockInfo Name="Red" DataValue="1"/>
  </Group>


  <Group Name="Wool">
    <OnBlockInfo BlockId="0x23" Note="blockInfoName"/>

    <DataValueRange NameList="White, Orange, Magenta, LightBlue, Yellow, Lime, Pink, Gray, LightGray, Cyan, Purple, Blue, Brown, Green, Red, Black"/>
  </Group>


  <Group Name="StainedClay">
    <OnBlockInfo BlockId="0x9F" Note="blockInfoName"/>
    <DataValueRange NameList="White, Orange, Magenta, LightBlue, Yellow, Lime, Pink, Gray, LightGray, Cyan, Purple, Blue, Brown, Green, Red, Black"/>
  </Group>


  <Group Name="StainedGlass">
    <OnBlockInfo BlockId="0x5F" Note="blockInfoName"/>
    <DataValueRange NameList="White, Orange, Magenta, LightBlue, Yellow, Lime, Pink, Gray, LightGray, Cyan, Purple, Blue, Brown, Green, Red, Black"/>
  </Group>


  <Group Name="StainedGlassPane">
    <OnBlockInfo BlockId="0xA0" Note="blockInfoName"/>
    <DataValueRange NameList="White, Orange, Magenta, LightBlue, Yellow, Lime, Pink, Gray, LightGray, Cyan, Purple, Blue, Brown, Green, Red, Black"/>
  </Group>


  <Group Name="Carpet">
    <OnBlockInfo BlockId="0xAB" Note="blockInfoName"/>
    <DataValueRange NameList="White, Orange, Magenta, LightBlue, Yellow, Lime, Pink, Gray, LightGray, Cyan, Purple, Blue, Brown, Green, Red, Black"/>
  </Group>


  <Group Name="Torch">
    <OnBlockInfo BlockId="0x32" Note="blockInfoName"/>
    <DataValueRange NameList="East, West, South, North, Standing" Start="1"/>
  </Group>


  <Group Name="RedstoneTorch">
    <OnBlockInfo Note="blockInfoName"/>
    <Group Name="Active">
      <OnBlockInfo BlockId="4C"/>
      <DataValueRange NameList="East, West, South, North, Standing" Start="1"/>
    </Group>
    <Group Name="Inactive">
      <OnBlockInfo BlockId="4B"/>
      <DataValueRange NameList="East, West, South, North, Standing" Start="1"/>
    </Group>
  </Group>


  <Group Name="Slab">
    <OnBlockInfo Note="blockInfoName"/>

    <Group Name="Stone">
      <BlockInfoCollection Key="type">
        <DataValueRange NameList="Normal, Sandstone, Wooden, Cobblestone, Brick, StoneBrick, NetherBrick, Quartz"/>
      </BlockInfoCollection>

      <Group Name="Single">
        <OnBlockInfo BlockId="0x2C"/>
        <Include Key="types"/>
      </Group>
      <Group Name="Double">
        <OnBlockInfo BlockId="0x2B"/>
        <Include Key="types"/>
        <BlockInfo Name="SmoothStone" DataValue="8"/>
        <BlockInfo Name="SmoothSandstone" DataValue="9"/>
        <BlockInfo Name="TileQuartz" DataValue="10"/>
      </Group>
    </Group>

    <Group Name="Wood">
      <BlockInfoCollection Key="type">
        <DataValueRange NameList="Oak, Spruce, Birch, Jungle, Acacia, DarkOak"/>
      </BlockInfoCollection>

      <Group Name="Single">
        <OnBlockInfo BlockId="0x7E"/>
        <Include Key="types"/>
      </Group>
      <Group Name="Double">
        <OnBlockInfo BlockId="0x7D"/>
        <Include Key="types"/>
      </Group>
    </Group>
  </Group>


  <Group Name="Fire">
    <OnBlockInfo BlockId="0x33"/>
    <For Start="0" Count="16">
      <Name><![CDATA['Tick' + ToString(i, 'X')]]></Name>
      <DataValue><![CDATA[i]]></DataValue>
    </For>
  </Group>


  <Group Name="Sandstone">
    <OnBlockInfo BlockId="0x18" Note="blockInfoName"/>
    <BlockInfo Name="Normal" DataValue="0"/>
    <BlockInfo Name="Chiseled" DataValue="1"/>
    <BlockInfo Name="Smooth" DataValue="2"/>
  </Group>


  <Group Name="Bed">
    <BlockInfoCollection Key="directions">
      <DataValueRange NameList="East, West, South, North"/>
    </BlockInfoCollection>

    <OnBlockInfo BlockId="0x1A"/>
    <Group Name="Foot">
      <Group Name="Unoccupied">
        <OnBlockInfo DataValue="dataValue + 0"/>
        <Include Key="directions"/>
      </Group>
      <Group Name="Occupied">
        <OnBlockInfo DataValue="dataValue + 4"/>
        <Include Key="directions"/>
      </Group>
    </Group>
    <Group Name="Head">
      <Group Name="Unoccupied">
        <OnBlockInfo DataValue="dataValue + 8"/>
        <Include Key="directions"/>
      </Group>
      <Group Name="Occupied">
        <OnBlockInfo DataValue="dataValue + 12"/>
        <Include Key="directions"/>
      </Group>
    </Group>
  </Group>


  <Group Name="TallGrass">
    <OnBlockInfo BlockId="0x1F"/>
    <DataValueRange NameList="Shrub, Normal, Fern, BiomeShrub"/>
  </Group>


  <Group Name="Flower">
    <Group Name="Small">
      <BlockInfo Name="Dandelion" BlockId="0x25" DataValue="0"/>
      <OnBlockInfo BlockId="0x26"/>
      <DataValueRange NameList="Poppy, BlueOrchid, Allium, AzureBluet, RedTulip, OrangeTulip, WhiteTulip, PinkTulip, OxeyeDaisy"/>
    </Group>
    <Group Name="Large">
      <OnBlockInfo BlockId="0xAF"/>
      <DataValueRange NameList="Sunflower, Lilac, DoubleTallgrass, LargeFern, RoseBush, Peony"/>
      <BlockInfo Name="Top" DataValue="8"/>
    </Group>
    <Group Name="Pot">
    </Group>
  </Group>


  <Group Name="Piston">
    <OnBlockInfo Note="GetGroupName(0) + ' ' + blockInfoName"/>

    <BlockInfoCollection Key="directions">
      <DataValueRange NameList="Down, Up, North, South, West, East"/>
    </BlockInfoCollection>

    <Group Name="Normal">
      <OnBlockInfo BlockId="0x21"/>
      <Group Name="Retracted">
        <OnBlockInfo DataValue="dataValue + 0"/>
        <Include Key="directions"/>
      </Group>
      <Group Name="Extended">
        <OnBlockInfo DataValue="dataValue + 8"/>
        <Include Key="directions"/>
      </Group>
    </Group>

    <Group Name="Sticky">
      <OnBlockInfo BlockId="0x1D"/>
      <Group Name="Retracted">
        <OnBlockInfo DataValue="dataValue + 0"/>
        <Include Key="directions"/>
      </Group>
      <Group Name="Extended">
        <OnBlockInfo DataValue="dataValue + 8"/>
        <Include Key="directions"/>
      </Group>
    </Group>

    <Group Name="Head">
      <OnBlockInfo BlockId="0x22"/>
