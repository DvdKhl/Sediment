  <Group Name="RedstoneWire">
    <OnBlockInfo BlockId="0x37"/>
    <For Start="0" Count="16">
      <Name><![CDATA['Power' + ToString(i, 'X')]]></Name>
      <DataValue><![CDATA[i]]></DataValue>
    </For>
  </Group>


  <Group Name="Farmland">
    <OnBlockInfo BlockId="0x3C"/>
